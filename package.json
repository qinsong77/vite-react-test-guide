{
  "name": "vite-react-test-guide",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier -c --write \"src/**/*.{ts,tsx}\" --end-of-line auto",
    "prepare": "husky install",
    "checklint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "reviselint": "eslint --fix --ext .js,.ts,.tsx  --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ],
    "*.{css,less}": [
      "stylelint --syntax less --fix"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@types/lodash": "^4.14.182",
    "@types/styled-components": "^5.1.25",
    "axios": "^0.27.2",
    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.2",
    "react-resize-detector": "^7.1.1",
    "react-router-dom": "6",
    "recharts": "^2.1.10",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-testing-library": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.7"
  }
}
